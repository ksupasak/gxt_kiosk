<%
	
	i = Device.find(params[:id])
	link = '../Home/index'
	if i
		session[:device_id] = i.id
		
	link = '.'
	link = "../Terminal/wizard?device_id=#{params[:id]}" if i.type=='Terminal'
	link = "../Kitchen/index?device_id=#{params[:id]}" if i.type=='Kitchen'
	link = "../Printer/index" if i.type=='Printer'
	link = "../Vending/index?device_id=#{params[:id]}" if i.type=='Vending'
	
	end

%>
<%=redirect_to link %>
